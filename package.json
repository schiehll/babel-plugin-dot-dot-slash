{
  "name": "babel-plugin-dot-dot-slash",
  "version": "1.0.0",
  "description": "Babel plugin to solve the '../../../' problem.",
  "author": "Reinaldo Schiehll <rn.schiehll@gmail.com> (http://github.com/schiehll)",
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/schiehll/babel-plugin-dot-dot-slash.git"
  },
  "keywords": [
    "babel",
    "babel-plugin",
    "import",
    "relative import"
  ],
  "scripts": {
    "test": "ava --no-cache",
    "build": "babel ./index.js --out-dir ./dist"
  },
  "ava": {
    "files": [
      "tests/**/*.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-preset-es2015": "^6.6.0"
  },
  "dependencies": {
    "app-root-path": "^1.0.0",
    "slash": "^1.0.0"
  }
}
